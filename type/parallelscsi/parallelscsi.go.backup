// Package parallelscsi provides data structures and initialization for system profiler's SPParallelSCSIDataType.
package parallelscsi

import (
	"sync"
	"fmt"
	"github.com/samburba/go-system-profiler/v2/internal/common"
)

// DataTypeItem represents the structure of SPParallelSCSIDataType.
type DataTypeItem struct {
	Name string `json:"_name"`
	// TODO: Add specific fields based on system_profiler output
}

// DataType holds the parsed system profiler data for SPParallelSCSIDataType.
var DataType *common.DataType[DataTypeItem]

func init() {
	// Ensure correct initialization of DataType.
	data, err := common.NewData[DataTypeItem](common.SPParallelSCSIDataType)
	if err != nil {
		fmt.Println("Error initializing data:", err)
		return
	}
	DataType = data
}
